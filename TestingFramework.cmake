# test for visualization of MueLu multigrid aggregates
macro(muelu_agg2vtk arg nlevel nblock nproc scaling)

  # set command to run Python script for converting *.out files with aggregation information from MueLu into Paraview-readable *.vtp files
  set(COMMANDS "python ${PROJECT_SOURCE_DIR}/tests/post_processing_test/muelu_agg2vtk.py ${PROJECT_SOURCE_DIR}/Input/${arg}.dat ${PROJECT_BINARY_DIR} ${nlevel} ${nproc} -p xxx_ -s ${scaling}")

  # subtract one from total numbers of multigrid levels and matrix blocks to obtain correct loop delimiters
  math(EXPR mlevel "${nlevel}-1")
  math(EXPR mblock "${nblock}-1")

  # set commands to compare *.vtp files generated by Python script with corresponding reference *.vtp files
  foreach(ilevel RANGE ${mlevel})
    foreach(iblock RANGE ${mblock})
      set(COMMANDS "${COMMANDS} && diff -q -s ${PROJECT_BINARY_DIR}/xxx_aggs_level${ilevel}_block${iblock}.vtp ${PROJECT_SOURCE_DIR}/Input/${arg}_aggs_level${ilevel}_block${iblock}.vtp")
    endforeach(iblock)
  endforeach(ilevel)

  # add test to testing framework
  add_test(NAME ${arg}-p${nproc}-agg2vtk COMMAND sh -c ${COMMANDS})

  # set maximum test runtime
  set_tests_properties(${arg}-p${nproc}-agg2vtk PROPERTIES TIMEOUT 100)

endmacro(muelu_agg2vtk)

# compare arbitrary result file to corresponding reference file
macro(result_file arg nproc filetag resultfilename referencefilename)

  # add test to testing framework
  add_test(NAME ${arg}-p${nproc}-${filetag} COMMAND diff -q -s ${PROJECT_BINARY_DIR}/${resultfilename} ${PROJECT_SOURCE_DIR}/Input/${referencefilename})

  # set maximum test runtime
  set_tests_properties(${arg}-p${nproc}-${filetag} PROPERTIES TIMEOUT 100)

endmacro(result_file)

