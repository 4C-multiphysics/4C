 void CONTACT::WearLagrangeStrategy::OutputWear()
 {
+#if 1
+  dserror("Linear solvers not included in contact module. Please contact AdCo Engineering GW GmbH for more details");
+#else
   if(weardiscr_)
   {
     if(weartimescales_)
@@ -4252,6 +4255,7 @@ void CONTACT::WearLagrangeStrategy::OutputWear()
       }
     }
   }
+#endif /* if 0 */
   return;
 }
 
