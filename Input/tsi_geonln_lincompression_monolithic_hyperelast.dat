ITERNORM                        L2
LINEAR_SOLVER                   3
MAXITER                         50
MAXTIME                         1
MINITER                         0
NLNSOL                          fullnewton  // vague,fullnewton,lsnewton,oppnewton,modnewton,nlncg,ptc,newtonlinuzawa,augmentedlagrange,NoxNewtonLineSearch,noxgeneral
NORMCOMBI_RESFDISP              And
NORM_DISP                       Abs
NORM_RESF                       Rel  // Abs,Mix
NUMSTEP                         200
PREDICT                         ConstDis  // Vague,ConstDis,ConstDisVelAcc,TangDis,ConstDisPres,ConstDisVelAccPres
RESULTSEVRY                     1
RESEVRYERGY                     0
RESTARTEVRY                     1
TIMESTEP                        1
TOLDISP                         1e-08
TOLRES                          1e-06
---------------------------------------------STRUCTURAL DYNAMIC/ONESTEPTHETA
THETA                           1.0
-------------------------------------------------------------THERMAL DYNAMIC
ADAPTCONV                       No
ADAPTCONV_BETTER                0.1
DIVERCONT                       stop
DYNAMICTYP                      OneStepTheta  // Statics,OneStepTheta
INITFUNCNO                      1
INITIALFIELD                    field_by_function
ITERNORM                        Rms  // L2,L1,Inf
LINEAR_SOLVER                   2
MAXITER                         50
MAXTIME                         1
MINITER                         0
NLNSOL                          fullnewton  // vague,fullnewton
NORMCOMBI_RESFTEMP              And
NORM_RESF                       Abs  // Mix,Rel,Abs
NORM_TEMP                       Abs
NUMSTEP                         200
PREDICT                         ConstTemp  // Vague,ConstTemp,ConstTempRate,TangTemp
RESULTSEVRY                     1
RESTARTEVRY                     1
TIMESTEP                        0.01
TOLRES                          1e-05
TOLTEMP                         1e-08
------------------------------------------------THERMAL DYNAMIC/ONESTEPTHETA
THETA                           1.0
-----------------------------------------------------------------TSI DYNAMIC
COUPALGO                        tsi_monolithic  // tsi_iterstagg,tsi_sequstagg,tsi_oneway,tsi_monolithic
ITEMIN                          1
ITEMAX                          10  // 100
MAXTIME                         1.0
NUMSTEP                         9  // 2000
NORM_INC                        Abs  // Rel
RESTARTEVRY                     1
TIMESTEP                        0.1
RESULTSEVRY                           1
------------------------------------------------------TSI DYNAMIC/MONOLITHIC
CONVTOL                         1e-07
TOLINC                          1e-08
ITERNORM                        Rms  // Rms,L2,Inf,L1_Scaled
LINEAR_SOLVER                   1
MERGE_TSI_BLOCK_MATRIX          Yes
NORMCOMBI_RESFINC               And  // And,Or,{{Coupl_}And,Or}_Singl
NORM_RESF                       Rel  //Abs,Rel,Mix
--------------------------------------------------------------------SOLVER 1
NAME                            TSI_MONOLITHIC_SOLVER
AZBDIAG                         0
